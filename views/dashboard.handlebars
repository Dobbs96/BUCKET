        <button href="#" id="deleteTrvlBtnHandlebars{{id}}" data-id="{{id}}" class="btn hoverable btn-small white-text amber darken-1">Delete</button>
      </div>
    </div>
  </div>
</div>

<!-- Modal Update Travel -->
<div id="modal{{id}}" class="modal">
  <div class="modal-content">
    <h4>Update: Travel</h4>
    <div class="row">
      <div class="travel-card-container">
      {{!-- <form class="col s12 new-post-travel" action="/api/travel/{{id}}" method="PUT"> --}}
      <form class="col s12 new-post-travel">
        <div class="row">
          <div class="input-field col s6">
            <input id="location" name="location" type="text" value="{{location}}" required />
            <label class="active" for="location">{{location}}</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <input id="budget" name="budget" type="text" value="{{budget}}" required/>
            <label class="active" for="budget">{{budget}}</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <input id="starting-date" name="starting_date" type="text" value="{{format_date starting_date}}" required/>
            <label class="active" for="starting_date">{{format_date starting_date}}</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <input id="ending-date" name="ending_date" type="text" value="{{format_date ending_date}}" required/>
            <label class="active" for="ending_date">{{format_date ending_date}}</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <input id="what-do" type="text" name="what_do" value="{{what_do}}" required/>
            <label class="active" for="description">{{what_do}}</label>
          </div>
        </div>
        <button
          class="btn waves-effect waves-light"
          type="submit"
          name="action"
          id="modaleUpdateTrvlBtn"
          data-id="{{id}}"
        >Update
          <i class="material-icons right">send</i>
        </button>
      </form>
      </div>
    </div>
  </div>
  <div class="modal-footer">
    <a href="#!" class="modal-close waves-effect waves-green btn-flat">Close</a>
  </div>
</div>
{{/each}}
</div>

