<div class="testContainerTravel">
{{#each travels as |travel|}}
<div class="row">
  <div class="col s12 m9">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">{{location}} </span>
        <div class="date-container">
          <div>{{format_date starting_date}} to</div>
          <div>{{format_date ending_date}}</div>
        </div>
        <p>Budget: ${{budget}}</p>
        <p>{{what_do}}</p>
      </div>
      <div class="card-action">
        <button href="#" id="updateTrvlBtnHandlebars{{id}}" data-id="{{id}}" data-location="{{location}}" data-budget="{{budget}}" data-whatdo="{{what_do}}" data-startingDate="{{starting_date}}" data-endingDate="{{ending_date}}" data-target="modal{{id}}" class="btn hoverable btn-small white-text amber darken-1 modal-trigger">Update</button>
